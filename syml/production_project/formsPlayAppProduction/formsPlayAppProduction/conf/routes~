# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)


#lead creation routes start from here ----------------------------------


GET     /clientref                    controllers.Application.indexNotVedio()

GET     /clientrefV                     controllers.Application.indexV()

POST     /leadcreation                           controllers.Application.submitReferral()


POST     /leadcreationV                           controllers.Application.submitReferralV()

#lead creation routes end here ----------------

#referral resoursce routes start here ------------------


GET   /referal             		 controllers.VisdomReferral.referral()
	
GET   /referalV           		  controllers.VisdomReferral.referralV()

GET   /wfg           		  controllers.VisdomReferral.wfg()
GET   /wfgv           		  controllers.VisdomReferral.wfgV()

GET   /realtor             		 controllers.VisdomReferral.realtor()

GET   /realtorV             	 controllers.VisdomReferral.realtorV()

GET   /professional          	 controllers.VisdomReferral.professional()

GET   /professionalV     	     controllers.VisdomReferral.professionalV()

GET   /planner             		controllers.VisdomReferral.planner()

GET   /plannerV              	controllers.VisdomReferral.plannerV()

GET   /client              		controllers.VisdomReferral.client()

GET   /clientV              	controllers.VisdomReferral.clientV()


POST   /referal1              	controllers.VisdomReferral.visdomReferral()
POST  /referral2     			controllers.VisdomReferral.visdomReferralForm2()


POST   /wfg1           		  controllers.VisdomReferral.wfg1()

POST   /wfg2           		  controllers.VisdomReferral.wfg2()




GET /bookmark     controllers.VisdomReferral.bookmark()
#referr resoursce routing ends here ------------------------------






